{
  "name": "clean_architecture_express",
  "version": "1.0.0",
  "main": "index.ts",
  "repository": "https://github.com/Blue-Cheesecake/Clean-Architecture-Express.git",
  "author": "Sinut Wattanarporn <s_w.piepie@outlook.com.au>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "prerelease-dev": "yarn run build",
    "release-dev": "NODE_ENV=dev node dist/index.js",
    "predebug-dev": "yarn run build",
    "debug-dev": "concurrently \"npx tsc -w\"  \"NODE_ENV=dev nodemon dist/index.js\"",
    "prerelease-stg": "yarn run build",
    "release-stg": "NODE_ENV=stg node dist/index.js",
    "predebug-stg": "yarn run build",
    "debug-stg": "concurrently \"npx tsc -w\"  \"NODE_ENV=stg nodemon dist/index.js\"",
    "prerelease-prd": "yarn run build",
    "release-prd": "NODE_ENV=prd node dist/index.js",
    "prewarch-prd": "yarn run build",
    "debug-prd": "concurrently \"npx tsc -w\"  \"NODE_ENV=prd nodemon dist/index.js\""
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "inversify": "^6.0.1",
    "mongoose": "^7.4.5",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.5.7",
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "tslint": "^6.1.3",
    "typescript": "^5.2.2"
  }
}
